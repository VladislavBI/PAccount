<div class="row" style="text-align:center;width: 100%;" >
    <div class="col-lg-3"> </div>
    <div class="col-lg-6"> <h3>Store money</h3></div>
    <div class="col-lg-3"> </div>
</div>
<br />
<form>
    <div class="row">
        <div class="col-lg-5"> <input type="number" ng-model="plan.newSum.Sum" /></div>
        <div class="col-lg-3">
            <select name="mySelect" id="mySelect" ng-model="plan.newSum.currency" ng-options="option.Name for option in plan.availableCurrency track by option.Id"></select>

        </div>
        <div class="col-lg-4"> <button ng-click="plan.addSum()">Add sum</button></div>
    </div>
</form>
<br />
<div class="row" style="width: 100%;">
    <div class="row" style="width: 100%;" >
        <div class="col-lg-1"> </div>
        <div class="col-lg-4"> Money stored</div>
        <div class="col-lg-3" ng-bind="plan.storedMoney"> </div>
        <div class="col-lg-1"> </div>
    </div>
</div>
<br />
