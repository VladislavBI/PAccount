<div class="row" style="text-align:center;width: 100%;" >
    <h2 style="width: 100%;">All planned buys</h2>
</div>
<div class="row" style="width: 100%;">
    <div class="col-lg-6 jsgrid-cell-imitate" >Name</div>
    <div class="col-lg-2 jsgrid-cell-imitate">Full sum</div>
    <div class="col-lg-2 jsgrid-cell-imitate">Sum left</div>
    <div class="col-lg-2 jsgrid-cell-imitate">Can buy</div>
</div>
<div class="row" ng-repeat="wish in plan.wishList" style="width: 100%;">
    <div class="col-lg-6 jsgrid-cell-imitate" ng-bind="wish.Name" ></div>
    <div class="col-lg-2 jsgrid-cell-imitate" ng-bind="wish.Sum" ></div>
    <div class="col-lg-2 jsgrid-cell-imitate" ng-bind="wish.Sum-plan.storedMoney" ></div>
    <div class="col-lg-2 jsgrid-cell-imitate"><input type="checkbox" ng-checked="wish.Sum<=plan.storedMoney" ng-disabled="true" /></div>
</div>
<br />
